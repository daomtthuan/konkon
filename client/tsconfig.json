{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["esnext", "esnext.asynciterable", "dom"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "typeRoots": ["./@types"],
    "types": ["@types/node", "@nuxt/types", "@nuxtjs/axios", "@types/nuxtjs__auth", "bootstrap-vue", "vue-class-component/hooks"]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
